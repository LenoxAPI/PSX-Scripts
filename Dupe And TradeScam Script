local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("LenHUBVR 0.6", "Ocean")
local player = game.Players.LocalPlayer
local runservice = game:GetService("RunService")


if game.PlaceId == 6284583030 then







-- Trade Scam
local Trade = Window:NewTab("Trade Scam")
local TradeSection = Trade:NewSection("Trading")

TradeSection:NewButton("TradeScam", "Click to TradeScam the other person.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/heUNtiqT", true))()
end)






--Dupe
local Dupe = Window:NewTab("Dupe Normal Pets")
local DupeSection = Dupe:NewSection("Duping")

DupeSection:NewTextBox("Pet Name", "Type the pet Name what you want to Dupe", function(txt)
	print(txt)
end)

DupeSection:NewTextBox("DupeAmount", "Type the amount for how often you want to dupe a Pet", function(txt)
	print(txt)
end)

DupeSection:NewButton("Start Duping", "Click when you entered Pet Name and Amount. Otherwise wont work", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/wuk7tGCF",true))()
end)



--Universal
local Universal = Window:NewTab("Universal")
local PlayerSection = Universal:NewSection("PlayerScripts")


PlayerSection:NewToggle("SuperHuman", "Makes the player have high speed and jumppower", function(state)
	if state then
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
	else
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	end
end)

PlayerSection:NewButton("Infinite Yield", "Admin CMDS", function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/Infiniteyield/master/source'),true))()
end)


PlayerSection:NewToggle("Fly", "Makes the player fly", function(state)
	if state then
		game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, true)
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
	else
		game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, false)
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
	end
end)

PlayerSection:NewToggle("Drunk", "Makes the player drunk", function(state)
	if state then
		character.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
	else
		character.Humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
	end
end)

PlayerSection:NewToggle("NoClip", "Makes the player noclip", function(state)
	if state then
		runservice.Stepped:Connect(function()
			for i,v in paris(player.Character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = false
				end
			end
		end)
	else
		runservice.Stepped:Connect(function()
			for i,v in paris(player.Character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = true
				end
			end
		end)
	end
	
	
	
PlayerSection:NewButton("Kill me", "Kills yourself", function()
	local humanoid = player.Character:FindFirstChild("Humanoid")
	humanoid.Health = 0
end)


end)


--Pet Sim X
local PetSim = Window:NewTab("PetSimX")
local MenuSection = PetSim:NewSection("Menus")
local player = game.Players.LocalPlayer
local bank = player.PlayerGui:FindFirstChild("Bank")

MenuSection:NewButton("Open Bank", "Opens The bank GUI", function()
	bank.Enabled = true
end)






--Info
local Info = Window:NewTab("Update & Info")
local UpdateSection = Info:NewSection("Updates")
local InfoSection = Info:NewSection("Information")

UpdateSection:NewLabel("Version 0.6")
InfoSection:NewLabel("Made With Love By:")
InfoSection:NewLabel("Lenox and JanVR")

end
